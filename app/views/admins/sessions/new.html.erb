

  <div class="login-box">
      <div class="login-logo">
        <a href="../../index2.html"><b>FRM</b></a>
      </div><!-- /.login-logo -->
      <div class="login-box-body">
        <p class="login-box-msg">Welcome To Admin Login</p>
       <%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="form-group has-feedback">
            <%= f.input :email, required: false, autofocus: true ,:class => 'form-control'%>
           
          </div>
          <div class="form-group has-feedback">
             <%= f.input :password, required: false ,:class => 'form-control'%>
           
          </div>
          <div class="row">
            
            <div class="col-xs-4">
            	 <%= f.button :submit, "Log in",:class=>"btn btn-primary btn-block btn-flat" %>
            
            </div><!-- /.col -->
          </div>
		<% end %>

     
        
      </div><!-- /.login-box-body -->
    </div><!-- /.login-box -->